definition
	CLUSTER[at0000] occurrences matches {0..*} matches {    -- Anatomical pathology examination
		items cardinality matches {1..*; unordered} matches {
			allow_archetype CLUSTER[at0027] occurrences matches {0..*} matches {    -- Specimen
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.specimen(-[a-zA-Z0-9_]+)*\.v1/}
			}
			ELEMENT[at0025] occurrences matches {0..1} matches {    -- Microscopic description
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0009] occurrences matches {0..*} matches {    -- Examination findings
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.laboratory_test_serology\.v0|openEHR-EHR-CLUSTER\.lab_microscopy_parasitology\.v0|openEHR-EHR-CLUSTER\.lab_antibody\.v0|openEHR-EHR-CLUSTER\.lab_antigen\.v0|openEHR-EHR-CLUSTER\.lab_microscopy_culture\.v0|openEHR-EHR-CLUSTER\.lab_microscopy_stain\.v0|openEHR-EHR-CLUSTER\.lab_molecular_microbial\.v0/}
			}
			allow_archetype CLUSTER[at0022] occurrences matches {0..*} matches {    -- Multimedia representation
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.media_file(-[a-zA-Z0-9_]+)*\.v1/}
			}
			ELEMENT[at0026] occurrences matches {0..1} matches {    -- Clinical interpretation
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0011] occurrences matches {0..1} matches {    -- Comment
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0028] occurrences matches {0..*} matches {    -- Examination not done
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.exclusion_test(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.exclusion_exam(-[a-zA-Z0-9_]+)*\.v1/}
			}
		}
	}

ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Anatomical pathology examination">
					description = <"A generic framework for findings observed during the histopathological or cytopathological examination of organs, tissues, cells, or body fluids using gross, microscopic, and ancillary testing methods.">
				>
				["at0009"] = <
					text = <"Examination findings">
					description = <"Structured details about the anatomical pathology examination findings.">
				>
				["at0011"] = <
					text = <"Comment">
					description = <"Additional narrative about the anatomical pathology examination findings, not captured in other fields.">
				>
				["at0022"] = <
					text = <"Multimedia representation">
					description = <"Digital image, video or diagram representing the anatomical pathology examination.">
					comment = <"Multiple formats are allowed but they should represent equivalent clinical content.">
				>
				["at0025"] = <
					text = <"Microscopic description">
					description = <"Narrative summary of findings observed under the microscope.">
				>
				["at0026"] = <
					text = <"Clinical interpretation">
					description = <"Single word, phrase or brief description that represents the clinical meaning and significance of the anatomical pathology examination for the identified specimen.">
					comment = <"For example: 'No abnormality detected' or 'Moderate inflammation present'. Coding of the 'Clinical interpretation' with a terminology is preferred, where possible.">
				>
				["at0027"] = <
					text = <"Specimen">
					description = <"Details about the specimen examined.">
					comment = <"For example, the ID of the specimen or a macroscopic description of the specimen being examined.">
				>
				["at0028"] = <
					text = <"Examination not done">
					description = <"Details to explicitly record that this examination was not performed.">
				>
			>
		>
