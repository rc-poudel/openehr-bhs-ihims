definition
	CLUSTER[at0000.1] matches {    -- Palpation of the cervix
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001.1] matches {    -- System or structure examined
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::at0.1]    -- Cervix
						}
					}
				}
			}
			ELEMENT[at0012] occurrences matches {0..1} matches {    -- Body site
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0011] occurrences matches {0..*} matches {    -- Structured body site
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.anatomical_location_circle(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.anatomical_location_relative(-[a-zA-Z0-9_]+)*\.v2/}
			}
			ELEMENT[at0003] occurrences matches {0..1} matches {    -- Clinical description
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0.2] occurrences matches {0..1} matches {    -- Relative position
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.3,    -- Anterior
							at0.4,    -- Normal
							at0.5,    -- Posterior
							at0.6,    -- Left
							at0.7]    -- Right
						}
					}
				}
			}
			ELEMENT[at0.8] occurrences matches {0..1} matches {    -- Consistency
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.9,    -- Firm
							at0.10,    -- Intermediate
							at0.11]    -- Soft
						}
					}
				}
			}
			ELEMENT[at0.12] occurrences matches {0..1} matches {    -- Surface description
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0.13] occurrences matches {0..1} matches {    -- Mobility
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0.14] occurrences matches {0..1} matches {    -- Tenderness
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0.15] occurrences matches {0..1} matches {    -- Thickness
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.16,    -- Thick
							at0.17,    -- Thin
							at0.18,    -- Paper-thin
							at0.19]    -- Oedematous
						}
					}
				}
			}
			ELEMENT[at0.20] occurrences matches {0..1} matches {    -- Effacement
				value matches {
					DV_PROPORTION matches {
						numerator matches {|0.0..100.0|}
						type matches {2}
					}
				}
			}
			ELEMENT[at0.21] occurrences matches {0..1} matches {    -- Cervical length
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"cm">
								magnitude = <|0.0..10.0|>
							>
						>
					>
				}
			}
			ELEMENT[at0.22] occurrences matches {0..1} matches {    -- Application to presenting part
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.24,    -- Loose
							at0.25]    -- Tight
						}
					}
				}
			}
			ELEMENT[at0.23] occurrences matches {0..1} matches {    -- Anterior lip presence
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.26,    -- Present
							at0.27]    -- Absent
						}
					}
				}
			}
			ELEMENT[at0.28] occurrences matches {0..1} matches {    -- External os dilation
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"cm">
								magnitude = <|0.0..10.0|>
							>
						>
					>
				}
			}
			allow_archetype CLUSTER[at0004] occurrences matches {0..*} matches {    -- Examination findings
				include
					archetype_id/value matches {/.*/}
			}
			ELEMENT[at0.29] occurrences matches {0..1} matches {    -- Internal os dilation
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"cm">
								magnitude = <|0.0..10.0|>
							>
						>
					>
				}
			}
			allow_archetype CLUSTER[at0005] occurrences matches {0..*} matches {    -- Multimedia representation
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.media_file(-[a-zA-Z0-9_]+)*\.v1/}
			}
			ELEMENT[at0006] occurrences matches {0..*} matches {    -- Clinical interpretation
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0007] occurrences matches {0..1} matches {    -- Comment
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0008] occurrences matches {0..*} matches {    -- Examination not done
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.exclusion_exam(-[a-zA-Z0-9_]+)*\.v1/}
			}
		}
	}

