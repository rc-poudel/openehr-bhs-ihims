definition
	CLUSTER[at0000.1] matches {    -- Examination of the uterus
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001.1] matches {    -- System or structure examined
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::at0.36]    -- Uterus
						}
					}
				}
			}
			ELEMENT[at0012] occurrences matches {0..1} matches {    -- Body site
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0011] occurrences matches {0..*} matches {    -- Structured body site
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.anatomical_location_circle(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.anatomical_location_relative(-[a-zA-Z0-9_]+)*\.v2/}
			}
			ELEMENT[at0003] occurrences matches {0..1} matches {    -- Clinical description
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0.1] occurrences matches {0..1} matches {    -- Length
				value matches {
					DV_QUANTITY matches {*}
				}
			}
			ELEMENT[at0.2] occurrences matches {0..1} matches {    -- Size estimation
				value matches {
					DV_DURATION matches {
						value matches {PW/|>=P0W|}
					}
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0.3] occurrences matches {0..1} matches {    -- Fundal height
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.4,    -- Not palpable
							at0.5,    -- At public symphsis
							at0.6,    -- At umbilicus
							at0.7]    -- At xiphisternum
						}
					}
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"cm">
							>
						>
					>
				}
			}
			ELEMENT[at0.8] occurrences matches {0..1} matches {    -- Tenderness
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.9,    -- Present
							at0.10]    -- Absent
						}
					}
				}
			}
			ELEMENT[at0.11] occurrences matches {0..1} matches {    -- Tenderness decription
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0.12] occurrences matches {0..1} matches {    -- Mobility
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.13,    -- Mobile
							at0.14,    -- LImited
							at0.15]    -- Fixed
						}
					}
				}
			}
			ELEMENT[at0.16] occurrences matches {0..1} matches {    -- Shape
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0.17] occurrences matches {0..1} matches {    -- Consistency
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0.18] occurrences matches {0..1} matches {    -- Version
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.19,    -- Anteverted
							at0.20,    -- Neutral
							at0.21]    -- Retroverted
						}
					}
				}
			}
			ELEMENT[at0.22] occurrences matches {0..1} matches {    -- Flexion
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.23,    -- Anteflexed
							at0.24,    -- Midflexed
							at0.25]    -- Retroflexed
						}
					}
				}
			}
			allow_archetype CLUSTER[at0004] occurrences matches {0..*} matches {    -- Examination findings
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.fetus_abdominal\.v0/}
			}
			allow_archetype CLUSTER[at0005] occurrences matches {0..*} matches {    -- Multimedia representation
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.media_file(-[a-zA-Z0-9_]+)*\.v1/}
			}
			ELEMENT[at0006] occurrences matches {0..*} matches {    -- Clinical interpretation
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0007] occurrences matches {0..1} matches {    -- Comment
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0008] occurrences matches {0..*} matches {    -- Examination not done
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.exclusion_exam(-[a-zA-Z0-9_]+)*\.v1/}
			}
		}
	}

ontology
	terminologies_available = <"SNOMED-CT", ...>
	term_definitions = <
		["en"] = <
			items = <
				["at0000.1"] = <
					text = <"Examination of the uterus">
					description = <"Findings observed during the palpation of the uterus.">
				>
				["at0.1"] = <
					text = <"Length">
					description = <"The estimated length from the fundus to the cervix.">
					comment = <"Measured in centimetres.">
				>
				["at0.2"] = <
					text = <"Size estimation">
					description = <"Size estimation of the uterus.">
					comment = <"An estimation of uterine size based on physical examination, documented as either a narrative description or a comparative estimate related to size of a pregnant uterus, such as '10 weeks'. These comparisons refer to the typical uterine size at specific stages of pregnancy and provide a standardised reference framework. Alternatively, locally recognised size descriptors, such as 'orange' or 'tennis ball' may be added to the DV_TEXT data type in a template to support a predefined list of terms.">
				>
				["at0.3"] = <
					text = <"Fundal height">
					description = <"The vertical distance from the symphysis pubis to the top of the pregnant uterine fundus.">
					comment = <"Usually only possible in pregnancy after approximately 20 weeks of gestation. This can be recorded as the measured distance or by identifying the height of the uterine fundus relative to identifiable anatomical landmarks, such as  the pubic symphysis, umbilicus, xiphoid process).">
				>
				["at0.4"] = <
					text = <"Not palpable">
					description = <"Uterus is not palpable in the abdomen.">
				>
				["at0.5"] = <
					text = <"At public symphsis">
					description = <"The uterus is just palpable at the level of the symphysis pubis.">
				>
				["at0.6"] = <
					text = <"At umbilicus">
					description = <"The uterus is palpable at, or about, the level of the umbilicus.">
				>
				["at0.7"] = <
					text = <"At xiphisternum">
					description = <"The uterus is palpable at, or about, the level of the xiphisternum.">
				>
				["at0.12"] = <
					text = <"Mobility">
					description = <"The degree to which the uterus can be displaced during bimanual examination, reflecting its freedom of movement within the pelvis.">
				>
				["at0.13"] = <
					text = <"Mobile">
					description = <"The uterus moves freely in response to gentle pressure during bimanual palpation, without resistance or pain.">
				>
				["at0.14"] = <
					text = <"LImited">
					description = <"The uterus can be displaced on bimanual palpation but with some resistance or reduced range of motion.">
				>
				["at0.15"] = <
					text = <"Fixed">
					description = <"The uterus does not move or is unable to be displaced on bimanual palpation.">
				>
				["at0.8"] = <
					text = <"Tenderness">
					description = <"The presence or absence of pain elicited during bimanual palpation of the uterus.">
				>
				["at0.9"] = <
					text = <"Present">
					description = <"Pain is elicited on bimanual palpation of the uterus.">
				>
				["at0.10"] = <
					text = <"Absent">
					description = <"No pain is elicited on bimanual palpation of the uterus.">
				>
				["at0.11"] = <
					text = <"Tenderness decription">
					description = <"Narrative description about findings of pain or tenderness of the uterus.">
					comment = <"For e.g. Tenderness present on anterior uterine wall, elicited during bimanual palpation, described by the patient as sharp and localized.">
				>
				["at0.16"] = <
					text = <"Shape">
					description = <"The overall anatomical contour of the uterus as assessed during bimanual palpation.">
